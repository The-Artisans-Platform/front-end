    "apollo-link-http": "^1.5.17",
    "axios": "^0.21.1",
    "axois": "^0.0.1-security",
    "cookie": "^0.4.1",
    "graphql": "^15.4.0",
    "isomorphic-unfetch": "^3.1.0",
    "moment": "^2.29.1",
    "next": "^10.0.5",
    "react": "^17.0.1",
    "react-apollo": "^3.1.5",
